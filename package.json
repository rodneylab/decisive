{
  "name": "decisive",
  "version": "0.0.1",
  "description": "front end for site admin",
  "author": "Rodney Lab <ask@rodneylab.com> (@rodneyLab)",
  "license": "BSD-3-Clause",
  "keywords": [
    "graphql",
    "blog",
    "svelte",
    "api",
    "typescript"
  ],
  "scripts": {
    "dev": "vite dev --port 3030 --https",
    "build": "vite build",
    "preview": "vite preview --port 3030 --https",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. .",
    "prettier:check": "prettier --check --plugin-search-dir=. .",
    "prepare": "husky install",
    "gen": "graphql-codegen --config codegen.yml && prettier --write src/lib/generated/graphql.ts",
    "test": "uvu tests  -r tsm -r module-alias/register -r vite-register -r tests/setup/register -i setup"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@fontsource/lato": "^4.5.9",
    "@graphql-codegen/cli": "2.11.6",
    "@graphql-codegen/introspection": "2.2.1",
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-operations": "2.5.3",
    "@iconify/svelte": "^2.2.1",
    "@rodneylab/sveltekit-components": "link:../components/package",
    "@sveltejs/kit": "next",
    "@testing-library/dom": "^8.17.1",
    "@testing-library/svelte": "^3.1.3",
    "@testing-library/user-event": "^14.4.3",
    "@types/js-search": "^1.4.0",
    "@types/leaflet": "^1.7.11",
    "@types/marked": "^4.0.5",
    "@types/node": "^18.7.6",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "@vitejs/plugin-basic-ssl": "^0.1.2",
    "dayjs": "^1.11.5",
    "dotenv": "^16.0.1",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "graphql": "^16.6.0",
    "graphql-codegen-svelte-apollo": "^1.1.0",
    "husky": "^8.0.1",
    "js-search": "^2.0.0",
    "jsdom": "^20.0.0",
    "just-throttle": "^4.1.1",
    "leaflet": "^1.8.0",
    "marked": "^4.0.18",
    "module-alias": "^2.2.2",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "slugify": "^1.6.5",
    "stemmer": "^2.0.0",
    "svelte": "^3.49.0",
    "svelte-calendar": "^3.1.6",
    "svelte-check": "^2.8.1",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.0",
    "tsm": "^2.2.2",
    "typescript": "^4.7.4",
    "u2f-api": "^1.2.1",
    "uvu": "^0.5.6",
    "vite": "^3.0.8",
    "vite-register": "^0.0.9"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rodneylab/decisive"
  },
  "bugs": {
    "url": "https://github.com/rodneylab/decisive/issues"
  },
  "type": "module",
  "dependencies": {
    "graphql-tag": "^2.12.6"
  }
}
