{
  "name": "decisive",
  "version": "0.0.1",
  "description": "front end for site admin",
  "author": "Rodney Lab <ask@rodneylab.com> (@rodneyLab)",
  "license": "BSD-3-Clause",
  "keywords": [
    "graphql",
    "blog",
    "svelte",
    "api",
    "typescript"
  ],
  "scripts": {
    "dev": "svelte-kit dev -p 3030 --https",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview -p 3030",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. .",
    "prettier:check": "prettier --check --plugin-search-dir=. .",
    "prepare": "husky install",
    "gen": "graphql-codegen --config codegen.yml && prettier --write src/lib/generated/graphql.ts"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@fontsource/lato": "^4.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@graphql-codegen/cli": "2.3.1",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.4.2",
    "@graphql-codegen/typescript-operations": "2.2.2",
    "@rodneylab/sveltekit-components": "link:../components/package",
    "@sveltejs/kit": "next",
    "@types/js-search": "^1.4.0",
    "@types/leaflet": "^1.7.8",
    "@types/node": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "dayjs": "^1.10.7",
    "dotenv": "^14.1.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.4.0",
    "graphql": "^15.8.0",
    "graphql-codegen-svelte-apollo": "^1.1.0",
    "husky": "^7.0.4",
    "js-search": "^2.0.0",
    "just-throttle": "^4.0.1",
    "leaflet": "^1.7.1",
    "marked": "^4.0.10",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.6.0",
    "slugify": "^1.6.5",
    "stemmer": "^2.0.0",
    "svelte": "^3.46.2",
    "svelte-awesome": "^2.4.2",
    "svelte-calendar": "^3.1.6",
    "svelte-check": "^2.3.0",
    "svelte-preprocess": "^4.10.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "u2f-api": "^1.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rodneylab/decisive"
  },
  "bugs": {
    "url": "https://github.com/rodneylab/decisive/issues"
  },
  "type": "module",
  "dependencies": {
    "graphql-tag": "^2.12.6"
  }
}
