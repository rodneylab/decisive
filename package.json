{
  "name": "decisive",
  "version": "0.0.1",
  "description": "front end for site admin",
  "author": "Rodney Lab <ask@rodneylab.com> (@rodneyLab)",
  "license": "BSD-3-Clause",
  "keywords": [
    "graphql",
    "blog",
    "svelte",
    "api",
    "typescript"
  ],
  "scripts": {
    "dev": "svelte-kit dev -p 3030 --https",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview -p 3030 --https",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. .",
    "prettier:check": "prettier --check --plugin-search-dir=. .",
    "prepare": "husky install",
    "gen": "graphql-codegen --config codegen.yml && prettier --write src/lib/generated/graphql.ts",
    "test": "uvu tests  -r tsm -r module-alias/register -r vite-register -r tests/setup/register -i setup"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@fontsource/lato": "^4.5.2",
    "@graphql-codegen/cli": "2.4.0",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.4.2",
    "@graphql-codegen/typescript-operations": "2.2.2",
    "@iconify/svelte": "^2.1.2",
    "@rodneylab/sveltekit-components": "link:../components/package",
    "@sveltejs/kit": "next",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/svelte": "^3.0.3",
    "@testing-library/user-event": "^13.5.0",
    "@types/js-search": "^1.4.0",
    "@types/leaflet": "^1.7.9",
    "@types/marked": "^4.0.2",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "dayjs": "^1.10.8",
    "dotenv": "^16.0.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "graphql": "^15.8.0",
    "graphql-codegen-svelte-apollo": "^1.1.0",
    "husky": "^7.0.4",
    "js-search": "^2.0.0",
    "jsdom": "^19.0.0",
    "just-throttle": "^4.0.1",
    "leaflet": "^1.7.1",
    "marked": "^4.0.12",
    "module-alias": "^2.2.2",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.6.0",
    "slugify": "^1.6.5",
    "stemmer": "^2.0.0",
    "svelte": "^3.46.4",
    "svelte-calendar": "^3.1.6",
    "svelte-check": "^2.4.5",
    "svelte-preprocess": "^4.10.4",
    "tslib": "^2.3.1",
    "tsm": "^2.2.1",
    "typescript": "^4.6.2",
    "u2f-api": "^1.2.1",
    "uvu": "^0.5.3",
    "vite-register": "^0.0.9"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rodneylab/decisive"
  },
  "bugs": {
    "url": "https://github.com/rodneylab/decisive/issues"
  },
  "type": "module",
  "dependencies": {
    "graphql-tag": "^2.12.6"
  }
}
